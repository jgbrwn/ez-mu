# Deny all access to this directory
# Protects SQLite database, cookies, and other sensitive files

# Apache 2.4+
<IfModule mod_authz_core.c>
    Require all denied
</IfModule>

# Apache 2.2
<IfModule !mod_authz_core.c>
    Order deny,allow
    Deny from all
</IfModule>

# Also prevent directory listing
Options -Indexes
